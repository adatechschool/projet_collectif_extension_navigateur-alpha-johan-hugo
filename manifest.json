{
    "manifest_version": 3,
    "name": "Test Extension",
    "version": "0.5",
    "permissions": [
        "notifications",
        "tabs",
        "activeTab"
      ],
    "content_scripts": [
        {
        "matches": [
            "https://parisbasketball.paris/*"
        ],
        "js": ["content.js"]  
        }
    ],
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
      },
      "background": {
        "service_worker": "background.js"
}
}